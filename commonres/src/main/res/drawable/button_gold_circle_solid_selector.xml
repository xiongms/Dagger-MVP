            <corners
                android:radius="30dp"
                android:bottomLeftRadius="30dp" android:bottomRightRadius="30dp" android:topLeftRadius="30dp" android:topRightRadius="30dp" />
            <solid android:color="@color/button_gray_disabled" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <corners
                android:radius="30dp"
                android:bottomLeftRadius="30dp" android:bottomRightRadius="30dp" android:topLeftRadius="30dp" android:topRightRadius="30dp" />
            <solid android:color="#bf9363" />
        </shape>
    </item>

    <item>
        <shape>
            <corners
                android:radius="30dp"
                android:bottomLeftRadius="30dp" android:bottomRightRadius="30dp" android:topLeftRadius="30dp" android:topRightRadius="30dp" />
            <solid android:color="#d7a671" />
        </shape>
    </item>
